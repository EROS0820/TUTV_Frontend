        android:id="@+id/ll_nav_support"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
