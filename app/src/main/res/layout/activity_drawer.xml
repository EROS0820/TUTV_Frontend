            />

    </LinearLayout>

    <!-- ll_nav_demo -->
    <LinearLayout
        android:id="@+id/ll_nav_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        android:visibility="gone"
        >
        <ImageView
            android:id="@+id/iv_demo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_demo_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_demo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/demo"
            android:textSize="@dimen/txt_size_h5"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- ll_nav_about -->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/ll_nav_about"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        >
        <ImageView
            android:id="@+id/iv_about"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_info_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_about"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/about"
