<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_nav_requester"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary2"
    android:orientation="vertical">


    <!-- ll_nav_vehicle -->
    <LinearLayout
        android:id="@+id/ll_nav_vehicle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        >
        <ImageView
            android:id="@+id/iv_vehicle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/home_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_vehicle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/home"
            android:textColor="@color/colorBlue1"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>


    <!-- ll_nav_home -->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/ll_nav_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/margin_7"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        >
        <ImageView
            android:id="@+id/iv_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/home_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tv_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/live_tracking"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- ll_nav_driver -->
    <LinearLayout
        android:id="@+id/ll_nav_driver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        >
        <ImageView
            android:id="@+id/iv_driver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_drive"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_driver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorBlue1"
            android:text="@string/driver"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>



    <!-- ll_nav_speedLimit -->
    <LinearLayout
        android:id="@+id/ll_nav_speedLimit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        >
        <ImageView
            android:id="@+id/iv_speedLimit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_timer"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_speedLimit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/speedLimit"
            android:textColor="@color/colorBlue1"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- ll_nav_device -->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/ll_nav_device"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        >
        <ImageView
            android:id="@+id/iv_device"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_device_number"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_device"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/device"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- ll_nav_history_replay -->
    <LinearLayout
        android:id="@+id/ll_nav_history_replay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        >
        <ImageView
            android:id="@+id/iv_history_replay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/clock_n"
            android:backgroundTint="@color/white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_history_replay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.TextNormal_White"
            android:textSize="@dimen/txt_size_h6"
            android:textColor="@color/colorBlue1"
            android:text="@string/history_replay"
            />

    </LinearLayout>


    <!-- ExpandableLinearLayout -->
    <!-- ll_nav_report -->
    <LinearLayout
        android:id="@+id/ll_nav_report"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relative_report"
            >
            <ImageView
                android:id="@+id/cv_thumbnail_head"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:maxHeight="25dp"
                android:maxWidth="25dp"

                android:background="@drawable/news_feed_w"
                android:layout_centerVertical="true"
                android:backgroundTint="@color/white"
                android:layout_gravity="center_vertical"
                android:layout_marginEnd="@dimen/margin_15"
                />

            <RelativeLayout
                android:id="@+id/btnUpDown_head"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:gravity="center"
                >
                <ImageView
                    android:id="@+id/ivUpDown"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/ic_arrow_left_white"
                    />
            </RelativeLayout>

            <TextView
                android:id="@+id/tv_report_head"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@id/btnUpDown_head"
                android:layout_toRightOf="@+id/cv_thumbnail_head"
                android:layout_centerVertical="true"
                android:paddingStart="@dimen/margin_10"
                style="@style/TextAppearance.TextNormal_White"
                android:textSize="@dimen/txt_size_h6"
                android:textColor="@color/colorBlue1"
                android:text="@string/report"
                />

        </RelativeLayout>

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayout_head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:ael_duration="400"
            app:ael_expanded="false"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <!-- ll_nav_distance_report -->
                <LinearLayout
                    android:id="@+id/ll_nav_distance_report"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingStart="@dimen/margin_30"
                    android:paddingTop="@dimen/margin_20"
                    android:paddingBottom="@dimen/margin_20"
                    >
                    <ImageView
                        android:id="@+id/iv_distance_report"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/location_start_point"
                        android:layout_gravity="center_vertical"
                        android:layout_marginEnd="@dimen/margin_15"
                        android:contentDescription="@string/support" />
                    <TextView
                        android:id="@+id/tv_distance_report"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/distance"
                        android:textSize="@dimen/txt_size_h6"
                        android:textColor="@color/colorBlue1"
                        style="@style/TextAppearance.TextNormal_White"
                        />

                </LinearLayout>

                <!-- ll_nav_trip_report -->
                <LinearLayout
                    android:id="@+id/ll_nav_trip_report"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingStart="@dimen/margin_30"
                    android:paddingBottom="@dimen/margin_20"
                    >
                    <ImageView
                        android:id="@+id/iv_trip_report"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_car_trip"
                        android:layout_gravity="center_vertical"
                        android:layout_marginEnd="@dimen/margin_15"
                        android:contentDescription="@string/support" />
                    <TextView
                        android:id="@+id/tv_trip_report"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/trip"
                        android:textColor="@color/colorBlue1"
                        android:textSize="@dimen/txt_size_h6"
                        style="@style/TextAppearance.TextNormal_White"
                        />

                </LinearLayout>

            </LinearLayout>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

    </LinearLayout>

    <ExpandableListView
        android:visibility="gone"
        android:id="@+id/expandableListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/nav_header_height"
        android:background="@color/white"
        android:dividerHeight="0dp"
        android:groupIndicator="@null" />

    <!-- RecyclerView -->
    <androidx.recyclerview.widget.RecyclerView
        android:visibility="gone"
        android:id="@+id/recycler_nav_report"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        />


    <!-- ll_nav_subUser -->
    <LinearLayout
        android:id="@+id/ll_nav_subUser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        >
        <ImageView
            android:id="@+id/iv_subUser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_user"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_subUser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/create_id"
            android:textSize="@dimen/txt_size_h6"
            android:textColor="@color/colorBlue1"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- ll_nav_complaint -->
    <LinearLayout
        android:id="@+id/ll_nav_complaint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        >
        <ImageView
            android:id="@+id/iv_complaint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_demo_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_complaint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/complaint"
            android:textSize="@dimen/txt_size_h6"
            android:textColor="@color/colorBlue1"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>


    <!-- ll_nav_toll -->
    <LinearLayout
        android:id="@+id/ll_nav_toll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        android:visibility="gone"
        >
        <ImageView
            android:id="@+id/iv_toll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/phone_grey"
            android:backgroundTint="@color/white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tv_toll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/toll_number"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- ll_nav_insurance -->
    <LinearLayout
        android:id="@+id/ll_nav_insurance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        android:visibility="gone"
        >
        <ImageView
            android:id="@+id/iv_insurance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_user"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tv_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/insurance"
            android:textSize="@dimen/txt_size_h6"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>

    <!-- roadSideAssistance -->
    <LinearLayout
        android:id="@+id/ll_nav_RSA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        >
        <ImageView
            android:id="@+id/iv_RSA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_user"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tv_RSA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rsa"
            android:textSize="@dimen/txt_size_h6"
            android:textColor="@color/colorBlue1"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>


    <!-- ll_nav_logout -->
    <LinearLayout
        android:id="@+id/ll_nav_logout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_15"
        >
        <ImageView
            android:id="@+id/iv_logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_info_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/logout2"
            android:textSize="@dimen/txt_size_h6"
            android:textColor="@color/colorBlue1"
            style="@style/TextAppearance.TextNormal_White"
            />

    </LinearLayout>



    <!-- ll_nav_geoFencing -->
    <LinearLayout
        android:id="@+id/ll_nav_geoFencing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/margin_30"
        android:paddingBottom="@dimen/margin_20"
        android:visibility="gone"
        >
        <ImageView
            android:id="@+id/iv_geoFencing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/location_white"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/margin_15"
            android:contentDescription="@string/support" />
        <TextView
            android:id="@+id/tv_geoFencing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/geoFencing"
            android:textSize="@dimen/txt_size_h6"
